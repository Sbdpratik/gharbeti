{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "property_type": {
      "type": "enumeration",
      "enum": [
        "Residental",
        "Commercial"
      ],
      "default": "Residental",
      "required": true
    },
    "property_categories": {
      "type": "enumeration",
      "enum": [
        "House",
        "Land",
        "Flat",
        "Business",
        "Apt. & Housing",
        "Room"
      ],
      "default": "Room"
    },
    "property_title": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "road_size": {
      "displayName": "Road Size",
      "type": "component",
      "repeatable": false,
      "component": "roadsize.road-size"
    },
    "building_details": {
      "type": "component",
      "repeatable": false,
      "component": "buildingdetails.building-details"
    },
    "total_area": {
      "displayName": "Total Area",
      "type": "component",
      "repeatable": false,
      "component": "totalarea.total-area"
    },
    "property_owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "properties"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Available",
        "Not Available"
      ],
      "default": "Available"
    },
    "property_tenants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "property"
    },
    "built_up_area": {
      "type": "component",
      "repeatable": false,
      "component": "builtuparea.built-up-area"
    },
    "property_face": {
      "type": "component",
      "repeatable": false,
      "component": "propertyface.property-face"
    },
    "no_of_room": {
      "type": "component",
      "repeatable": false,
      "component": "noofroom.no-of-rooms"
    },
    "amenities": {
      "type": "component",
      "repeatable": false,
      "component": "amenities.amenities"
    },
    "description": {
      "type": "component",
      "repeatable": false,
      "component": "propertydescription.property-description"
    },
    "owner_details": {
      "type": "component",
      "repeatable": false,
      "component": "ownerdetails.owner-details"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "component",
      "repeatable": false,
      "component": "pricedetails.price-details"
    },
    "rental_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rental-detail.rental-detail",
      "mappedBy": "property"
    }
  }
}
