{
  "kind": "collectionType",
  "collectionName": "rooms",
  "info": {
    "singularName": "room",
    "pluralName": "rooms",
    "displayName": "Room",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "default": "unnamed",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "dimension": {
      "type": "component",
      "repeatable": false,
      "component": "dimension.dimension"
    },
    "floor": {
      "type": "integer"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal",
      "min": 1000
    },
    "electricity": {
      "default": 15,
      "type": "decimal"
    },
    "water": {
      "type": "decimal"
    },
    "sanitation": {
      "type": "decimal"
    },
    "solar": {
      "type": "decimal"
    },
    "internet": {
      "type": "decimal"
    },
    "total": {
      "type": "decimal"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "rooms"
    }
  }
}
